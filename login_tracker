users = ['john','charlie','bob', 'charlie', 'bob', 'john', 'alex', 'prp' , 'charlie']

logins = {}

for username in users:
    if username in logins:
        logins[username] += 1
    else:
        logins[username] = 1
for user ,count in logins.items():
    print(f'{user:8}:{count} logins ')